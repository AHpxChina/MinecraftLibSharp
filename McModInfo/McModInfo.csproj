<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Authors>Liam Sho</Authors>
    <Company>UnusualDream</Company>
    <Product>Minecraft Mod Info Reader</Product>
    <Description>Read mcmod.info file in a Minecraft Mod Jar.
Visit my github for more infomation.</Description>
    <Version>1.0.1</Version>
    <PackageProjectUrl>https://github.com/LiamSho/MinecraftLibSharp</PackageProjectUrl>
    <PackageId>LiamSho.MinecraftLibSharp.McModInfo</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="SharpZipLib" Version="1.2.0" />
  </ItemGroup>

</Project>
